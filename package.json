{
  "name": "salt",
  "version": "2.0.7",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run start:develop",
    "start:develop": "NODE_ENV=develop nodemon build/index.js",
    "start:production": "NODE_ENV=production node build/index.js",
    "start:staging": "NODE_ENV=staging node build/index.js",
    "test": "LOG_LEVEL=warn npx jest __test__ --detectOpenHandles --notify",
    "test:ci": "npx jest __ci__ --detectOpenHandles --notify",
    "test:watch": "npm test -- --watch",
    "ut": "LOG_LEVEL=warn npx jest --detectOpenHandles --notify",
    "utw": "npm run ut -- --watch",
    "coverage": "npm test -- --coverage",
    "build": "npx babel src --out-dir build",
    "build:watch": "nodemon --ext js --watch src --exec 'npm run build'",
    "lint": "npx eslint src __test__ __ci__",
    "doc": "npx esdoc",
    "plato": "npx plato -r -n -e .eslintrc.json -t 'Salt Analysis Report' -d doc/plato src",
    "report": "npm run plato; npm run doc; npm run coverage"
  },
  "author": "FAST RETAILING CO., LTD.",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@google-cloud/datastore": "^3.1.1",
    "@google-cloud/storage": "~2.3.4",
    "bcryptjs": "^2.4.3",
    "bunyan": "^1.8.12",
    "dotenv": "^6.2.0",
    "edgegrid": "^3.0.6",
    "fluent-ffmpeg": "^2.1.2",
    "fs": "0.0.1-security",
    "mime": "^2.4.2",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.23",
    "restify": "^7.7.0",
    "uuid": "^3.3.2",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "csv-parse": "^4.4.0",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.16.0",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "node-fetch": "^2.3.0",
    "nodemon": "^1.18.11",
    "plato": "^1.7.0"
  },
  "nodemonConfig": {
    "ignore": [
      "*.spec.js",
      "*.test.js"
    ],
    "delay": "2500"
  },
  "husky": {
    "hooks": {
      "post-merge": "npm install"
    }
  },
  "jest": {
    "verbose": true,
    "setupFilesAfterEnv": [
      "<rootDir>/__test__/setupTests.js"
    ],
    "moduleNameMapper": {
      "^src/(.*)": "<rootDir>/src/$1"
    },
    "coverageDirectory": "doc/coverage"
  }
}
